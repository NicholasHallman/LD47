pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
function _init()
	l=1
	r=2
	w=0
	d=r
	
	cx=64
	cy=64
	c_size=16
	
	sw=false
	sw_off=c_size/2

	ju=false
	grv=1

	w_t=0
	sw_t=0
	ju_t=0
end

function _update60()
	if(btn(1)) then
		w=r
		d=r
		cx+=1
	elseif(btn(0)) then
		w=l
		d=l
		cx-=1
	else
		w=0
	end
	
	if(btnp(4)) then ju=true end
	if(btnp(5)) then sw=true end
end

function _draw()
	cls()
	c_frame()
	if(sw) sw_frame()
end

function c_frame()
	s=0
	if(w>0) then
		if(w_t>10) then s=1 end

		w_t+=1
		if(w_t>=20) then w_t=0 end		
	end
	
	if(ju) then
		s=2
		ju_t+=1
		if(ju_t>30) then
			ju_t=0
			ju=false
		end
	end
	
	spr(s,cx,cy,1,1,d==l,false)
end

function sw_frame()
	s=3
	for i=1, 4 do
		if (sw_t<i*2) then
			s+=(i-1)
			break
		end
	end
	sw_t+=1
	if(sw_t>=8) then
		sw_t=0
		sw=false
	end
	
	flp=d==r
	sx=cx-sw_off
	if(flp) then
		sx=cx+sw_off
	end
	sy=cy
	spr(s,sx,sy,1,1,flp,false)
end
